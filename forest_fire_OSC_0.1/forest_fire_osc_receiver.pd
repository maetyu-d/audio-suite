#X floatatom 620 310 6 0 0 0 rain_state - -;
#X floatatom 696 310 10 0 0 0 rain_value - -;
#X obj 24 350 sel 1;
#X obj 220 350 sel 1;
#X obj 420 350 sel 1;
#X obj 620 350 sel 1;
#X obj 24 380 bng 15 250 50 0 empty empty burning_bang 0 -6 0 8 -262144 -1 -1;
