#X obj 420 380 bng 15 250 50 0 empty empty embers_bang 0 -6 0 8 -262144 -1 -1;
#X obj 620 380 bng 15 250 50 0 empty empty rain_bang 0 -6 0 8 -262144 -1 -1;
