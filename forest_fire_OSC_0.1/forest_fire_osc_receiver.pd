#N canvas 110 80 980 640 10;
#X text 24 10 Forest Fire OSC Receiver (Pure Data Vanilla);
#X text 24 30 Uses [netreceive -u -b] + [oscparse];
#X text 24 50 Listens on UDP port 9000 (change below if needed);
#X obj 24 84 netreceive -u -b 9000;
#X obj 24 116 oscparse;
#X obj 24 148 list trim;
#X obj 24 180 route fire;
#X obj 24 214 route burning_hi ignitions_spike embers_spike rain;
#X text 24 246 Each message is: state value;
#X obj 24 276 unpack f f;
#X obj 220 276 unpack f f;
#X obj 420 276 unpack f f;
#X obj 620 276 unpack f f;
#X floatatom 24 310 6 0 0 0 burning_state - -;
